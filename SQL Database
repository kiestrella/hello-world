Members
CREATE TABLE Members
( MemberID CHAR (5),
  FName CHAR (20),
  LName CHAR (20),
  Phone CHAR (15),
  EmailAddress CHAR (32),
  AddressID CHAR (5),
  PRIMARY KEY (MemberID),
  FOREIGN KEY (AddressID) REFERENCES Addresses(AddressID)
);
 
