Orders
CREATE TABLE Orders
( OrderID CHAR(5),
  PaymentMethod CHAR(15),
  SalesTax CHAR(15),
  TotalAmt DECIMAL (7, 2),           	
  PRIMARY KEY(OrderID)
);
 
