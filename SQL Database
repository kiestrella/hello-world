Order details
CREATE TABLE OrderDetails
( OrderID CHAR (5),
  ISBN CHAR (17),
  EmailAddress CHAR (32),
  OrderDate DATE,
  OrderTime CHAR(8),
  Quantity INT,
  DiscountApplied CHAR (20),
  AddressID CHAR (5),
  MemberID CHAR (5),
  PRIMARY KEY (OrderID, ISBN),
  FOREIGN KEY (AddressID) REFERENCES Addresses(AddressID),
  FOREIGN KEY (MemberID) REFERENCES Members(MemberID)
 );
 
 
