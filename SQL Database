Addresses
CREATE TABLE Addresses
( AddressID CHAR(5),
  StAddress CHAR(50),
  ZIPCode CHAR(5),
PRIMARY KEY (AddressID),
FOREIGN KEY (ZipCode) REFERENCES Zip(Zipcode)
);

