Employees
CREATE TABLE Employees
( EmployeeID CHAR(5),
  FName CHAR(15),
  LName CHAR(15),
  DOB DATE,
  Phone CHAR(13),
  Email CHAR(25),
  DepartmentID CHAR(5),
  AddressID CHAR(5),
  PRIMARY KEY(EmployeeID),
  FOREIGN KEY (DepartmentID) REFERENCES Departments(DepartmentID),
  FOREIGN KEY (AddressID) REFERENCES Addresses(AddressID)
);
 
