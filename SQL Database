Publishers
CREATE TABLE Publishers
( PublisherID CHAR(5),
  PublisherName CHAR(30),
  Phone CHAR(13), 	
  Email CHAR(25),
  ZipCode CHAR(5),
  PRIMARY KEY(PublisherID),
  FOREIGN KEY (ZipCode) REFERENCES Zip(Zipcode)
);
 
