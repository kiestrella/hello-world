CREATE TABLE Books
( ISBN CHAR (17),
  BookTitle CHAR (50),
  BookCategory CHAR (20),
  Genre CHAR (24),
  DepartmentID CHAR (5),
  AuthorID CHAR (5),
  PublisherID CHAR (5),
  PRIMARY KEY (ISBN),
  FOREIGN KEY (AuthorID) REFERENCES Authors (AuthorID),
  FOREIGN KEY (DepartmentID) REFERENCES Departments(DepartmentID),
  FOREIGN KEY (PublisherID) REFERENCES Publishers(PublisherID)
 );
 
