Shipping Details
CREATE TABLE ShippingDetails
( OrderId CHAR(5),
  AddressID CHAR(5),
  ShipMethod CHAR(20),   
  ShipDate DATE,
  DeliveryDate DATE,
  ShipCost DECIMAL(4,2), 	
 FOREIGN KEY (AddressID) REFERENCES Addresses(AddressID),
 Primary Key (OrderID, AddressID, ZIPCode)
);
 
